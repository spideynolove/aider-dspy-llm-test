{
    "project_name": "financial_data_scraper",
    "description": "Production-grade financial data scraping system",
    "data_type": "OHLCV",
    "website": {
        "url": "https://example.com",
        "structure": {
            "pagination": "API-based",
            "dynamic_content": true,
            "authentication": false,
            "rate_limits": {
                "requests_per_second": 5,
                "burst_capacity": 10
            }
        }
    },
    "output": {
        "format": "JSON",
        "fields": [
            "open",
            "high",
            "low",
            "close",
            "volume",
            "timestamp",
            "source",
            "metadata"
        ],
        "storage": {
            "primary": "MongoDB",
            "secondary": "PostgreSQL",
            "archive": "S3"
        },
        "data_validation": {
            "required_fields": ["open", "high", "low", "close", "volume", "timestamp"],
            "numeric_fields": ["open", "high", "low", "close", "volume"],
            "timestamp_format": "ISO8601"
        }
    },
    "settings": {
        "concurrency": 8,
        "download_delay": 1,
        "retry_times": 3,
        "timeout": 30,
        "middleware": {
            "proxy_rotation": true,
            "user_agent_rotation": true,
            "retry_policy": {
                "max_retry_times": 5,
                "retry_http_codes": [500, 502, 503, 504, 408, 429]
            }
        },
        "monitoring": {
            "stats_collection": true,
            "error_tracking": true,
            "performance_metrics": true
        },
        "logging": {
            "level": "INFO",
            "rotation": "daily",
            "retention": "7d"
        }
    },
    "testing": {
        "unit_tests": true,
        "integration_tests": true,
        "mock_responses": true
    },
    "deployment": {
        "docker": true,
        "kubernetes": false,
        "scaling": {
            "horizontal": true,
            "vertical": false
        }
    }
}
